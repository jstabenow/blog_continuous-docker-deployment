{
    "id": "jenkins-master",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "10.0.100.61:31000/devcluster/jenkins-master-1"
        },
        "volumes": []
    },
    "env": {
        "JENKINS_HOME": "/var/lib/jenkins",
        "MESOS_ZK": "10.0.100.61:2181,10.0.100.62:2181,10.0.100.63:2181"
    },
    "cpus": 4,
    "mem": 8192,
    "instances": 1,
    "ports": [
        31101,
        31102
    ],
    "requirePorts": true,
    "constraints": [
        [
            "hostname",
            "CLUSTER",
            "ac1.dev"
        ]
    ],
    "healthChecks": [
        {
            "path": "/",
            "portIndex": 0,
            "protocol": "HTTP",
            "gracePeriodSeconds": 30,
            "intervalSeconds": 30,
            "timeoutSeconds": 30,
            "maxConsecutiveFailures": 3
        }
    ],
    "upgradeStrategy": {
        "minimumHealthCapacity": 0
    }
}